  //convert list to string
    listno.joinToString(separator = ",") -- will return list as string seperated by  comma
 //convert string to list
    val list: List<String> = b.split(",").toList()
  //declare list
    val list = listOf(1,2,3)
  //check element in list
    if (5 in list) {
        println("true")
    }else {
        println("false")
    }
  //check list is empty or not
    list.isNotEmpty()
  //difference between two lists in kotlin
    val first = listOf(1, 2, 3, 4, 5)
    val second = listOf(1, 2, 3)
    val difference = first.minus(second)    // [4, 5]
//add two lists 
  var a = mutableListOf(1,2,3)
    var b = mutableListOf(4,5,6)
    var r = mutableListOf<Int>()
    r.addAll(a)
    r.addAll(b)
